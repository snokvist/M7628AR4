// M7628AR4 (MT7628AN 2.4 GHz + MT7663E PCIe 5 GHz)
// Factory partition contains EEPROM @ 0x0 and MACs at 0x04/0x28/0x2E.

#include "mt7628an.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "custom,m7628ar4", "mediatek,mt7628an-soc";
	model = "Custom M7628AR4 (MT7628AN + MT7663E)";

	aliases {
		label-mac-device = &ethernet;
	};

	chosen {
		/* Use your vendor’s UART default so you see serial immediately. */
		bootargs = "console=ttyS1,57600";
	};
};

/* 16 MiB SPI-NOR as per your /proc/mtd:
   0x000000..0x030000 : Bootloader (192 KiB)
   0x030000..0x040000 : Config     (64 KiB)
   0x040000..0x050000 : Factory    (64 KiB)
   0x050000..0x1000000: firmware   (rest)
*/
&spi0 {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <50000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			uboot: partition@0 {
				label = "Bootloader";
				reg = <0x000000 0x030000>;
				read-only;
			};

			config: partition@30000 {
				label = "Config";
				reg = <0x030000 0x010000>;
				read-only;
			};

			factory: partition@40000 {
				label = "Factory";
				reg = <0x040000 0x010000>;
				read-only;
			};

			firmware: partition@50000 {
				label = "firmware";
				reg = <0x050000 0xFB0000>;
			};
		};
	};
};

/* Internal Fast Ethernet MAC – label MAC at factory@0x28 */
&ethernet {
	status = "okay";
	mtd-mac-address = <&factory 0x28>;
};

/* Internal 2.4 GHz (mt76) – EEPROM at factory@0x0 */
&wmac {
	status = "okay";
	mediatek,mtd-eeprom = <&factory 0x0>;
};

/* PCIe root for the MT7663E 5 GHz card */
&pcie {
	status = "okay";
};
